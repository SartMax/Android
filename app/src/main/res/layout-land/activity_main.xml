<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/buttonASin"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:background="#00BCD4"
        android:textSize="50px"
        android:text="ASIN"
        app:layout_constraintBottom_toTopOf="@+id/buttonACos"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/buttonSin" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/buttonAC"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="#00BCD4"
        android:text="AC"
        android:textSize="50px"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/buttonPoint"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="#00BCD4"
        android:text="."
        android:textSize="50px"
        app:layout_constraintBottom_toTopOf="@+id/buttonAC"
        app:layout_constraintStart_toStartOf="parent" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/buttonSeven"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="#00BCD4"
        android:text="7"
        android:textSize="50px"
        app:layout_constraintBottom_toTopOf="@+id/buttonPoint"
        app:layout_constraintStart_toStartOf="parent" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/buttonFour"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="#00BCD4"
        android:text="4"
        android:textSize="50px"
        app:layout_constraintBottom_toTopOf="@+id/buttonSeven"
        app:layout_constraintStart_toStartOf="parent" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/buttonOne"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="#00BCD4"
        android:text="1"
        android:textSize="50px"
        app:layout_constraintBottom_toTopOf="@+id/buttonFour"
        app:layout_constraintStart_toStartOf="parent" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/buttonDelete"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="#00BCD4"
        android:text="DELETE"
        android:textSize="50px"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/buttonAC" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/buttonNull"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="#00BCD4"
        android:text="0"
        android:textSize="50px"
        app:layout_constraintBottom_toTopOf="@+id/buttonDelete"
        app:layout_constraintStart_toEndOf="@+id/buttonPoint" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/button8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="#00BCD4"
        android:text="8"
        android:textSize="50px"
        app:layout_constraintBottom_toTopOf="@+id/buttonNull"
        app:layout_constraintStart_toEndOf="@+id/buttonSeven" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/buttonFive"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="#00BCD4"
        android:text="5"
        android:textSize="50px"
        app:layout_constraintBottom_toTopOf="@+id/button8"
        app:layout_constraintStart_toEndOf="@+id/buttonFour" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/buttonTwo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="#00BCD4"
        android:text="2"
        android:textSize="50px"
        app:layout_constraintBottom_toTopOf="@+id/buttonFive"
        app:layout_constraintStart_toEndOf="@+id/buttonOne" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/buttonPersent"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="#00BCD4"
        android:text="%"
        android:textSize="50px"
        app:layout_constraintBottom_toTopOf="@+id/buttonEqually"
        app:layout_constraintStart_toEndOf="@+id/buttonNull" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/buttonNine"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="#00BCD4"
        android:text="9"
        android:textSize="50px"
        app:layout_constraintBottom_toTopOf="@+id/buttonPersent"
        app:layout_constraintStart_toEndOf="@+id/button8" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/buttonSix"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="#00BCD4"
        android:text="6"
        android:textSize="50px"
        app:layout_constraintBottom_toTopOf="@+id/buttonNine"
        app:layout_constraintStart_toEndOf="@+id/buttonFive" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/buttonThree"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="#00BCD4"
        android:text="3"
        android:textSize="50px"
        app:layout_constraintBottom_toTopOf="@+id/buttonSix"
        app:layout_constraintStart_toEndOf="@+id/buttonTwo" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/buttonEqually"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:background="#00BCD4"
        android:text="="
        android:textSize="30dp"
        app:layout_constraintBottom_toBottomOf="parent"

        app:layout_constraintStart_toEndOf="@+id/buttonDelete" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/buttonPlus"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:background="#00BCD4"
        android:text="+"
        android:textSize="50px"
        app:layout_constraintBottom_toTopOf="@+id/buttonEqually"

        app:layout_constraintStart_toEndOf="@+id/buttonPersent" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/buttonMinus"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:background="#00BCD4"
        android:text="-"
        android:textSize="50px"
        app:layout_constraintBottom_toTopOf="@+id/buttonPlus"

        app:layout_constraintStart_toEndOf="@+id/buttonNine" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/buttonMultiplay"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:background="#00BCD4"
        android:text="*"
        android:textSize="50px"
        app:layout_constraintBottom_toTopOf="@+id/buttonMinus"

        app:layout_constraintStart_toEndOf="@+id/buttonSix" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/buttonDivide"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:background="#00BCD4"
        android:text="/"
        android:textSize="50px"
        app:layout_constraintBottom_toTopOf="@+id/buttonMultiplay"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/buttonThree" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/buttonSqrt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:background="#00BCD4"
        android:text="SQRT"
        android:textSize="50px"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/buttonEqually"
        app:layout_constraintTop_toBottomOf="@+id/buttonPlus" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/buttonPI"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="#00BCD4"
        android:text="PI"
        android:textSize="50px"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/buttonSqrt" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/buttonE"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="#00BCD4"
        android:text="E"
        android:textSize="50px"
        app:layout_constraintBottom_toTopOf="@+id/buttonPI"
        app:layout_constraintStart_toEndOf="@+id/buttonPlus" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/buttonDegree"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="#00BCD4"
        android:text="^"
        android:textSize="50px"
        app:layout_constraintBottom_toTopOf="@+id/buttonE"
        app:layout_constraintStart_toEndOf="@+id/buttonMinus" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/buttonRightbracket"
        android:layout_width="88dp"
        android:layout_height="wrap_content"
        android:background="#00BCD4"
        android:text=")"
        android:textSize="50px"
        app:layout_constraintBottom_toTopOf="@+id/buttonDegree"
        app:layout_constraintStart_toEndOf="@+id/buttonMultiplay" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/buttonLeftbracket"
        android:layout_width="88dp"
        android:layout_height="wrap_content"
        android:background="#00BCD4"
        android:text="("
        android:textSize="50px"
        app:layout_constraintBottom_toTopOf="@+id/buttonRightbracket"
        app:layout_constraintStart_toEndOf="@+id/buttonDivide" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/buttonFact"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:background="#00BCD4"
        android:text="FACT"
        android:textSize="50px"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/buttonPI" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/buttonLog"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:background="#00BCD4"
        android:text="LOG"
        android:textSize="50px"
        app:layout_constraintBottom_toTopOf="@+id/buttonFact"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/buttonE" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/buttonTg"
        android:layout_width="88dp"
        android:layout_height="wrap_content"
        android:background="#00BCD4"
        android:text="TG"
        android:textSize="50px"
        app:layout_constraintBottom_toTopOf="@+id/buttonLog"
        app:layout_constraintStart_toEndOf="@+id/buttonDegree" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/buttonCos"
        android:layout_width="88dp"
        android:layout_height="wrap_content"
        android:background="#00BCD4"
        android:text="COS"
        android:textSize="50px"
        app:layout_constraintBottom_toTopOf="@+id/buttonTg"
        app:layout_constraintStart_toEndOf="@+id/buttonRightbracket" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/buttonSin"
        android:layout_width="88dp"
        android:layout_height="wrap_content"
        android:background="#00BCD4"
        android:text="SIN"
        android:textSize="50px"
        app:layout_constraintBottom_toTopOf="@+id/buttonCos"
        app:layout_constraintStart_toEndOf="@+id/buttonLeftbracket" />

    <TextView
        android:id="@+id/textviewout"
        android:layout_width="match_parent"
        android:paddingLeft="20dp"
        android:layout_height="50dp"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textviewinput"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:paddingLeft="20dp"
        android:layout_marginBottom="20dp"
        app:layout_constraintBottom_toTopOf="@+id/buttonDivide"
        tools:layout_editor_absoluteX="-16dp" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/buttonATg"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:background="#00BCD4"
        android:textSize="50px"
        android:text="ATG"
        app:layout_constraintBottom_toTopOf="@+id/buttonLog"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/buttonTg" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/buttonACos"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:background="#00BCD4"
        android:textSize="50px"
        android:text="ACOS"
        app:layout_constraintBottom_toTopOf="@+id/buttonATg"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/buttonCos" />

</androidx.constraintlayout.widget.ConstraintLayout>